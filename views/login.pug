doctype html
head
  meta(charset='UTF-8')
  title User Login
  link(href='../public/css/login_style.css' rel='stylesheet')
  script(src='../public/js/register.js' defer='')
form.register
  #login
    h1 Negative Impact
    label(for='email') Email
    input#email(type='email' name='email')
    label(for='password') Password
    input#password(type='password' name='password')
    input#submit(type='submit' value='Register')
    p.createaccount
      a(href='register') Create Account
