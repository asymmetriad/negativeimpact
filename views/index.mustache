<div id="h2title">
    <h2>Negative Impact</h2>
</div>

<body>
    <div>
        <form id="travel">
            <label for="distance">Distance traveled: </label>
            <input type="text" name="distance" placeholder="Enter distance traveled here..."><br>
            <label for="traveltype"> Type of travel used: </label>
            <select form="travel" name="traveltype">{{#options}}
                    <option value="{{value}}" {{#selected}}selected{{/selected}}>
                        {{text}}
                    </option>{{/options}}
            </select>
            <input type="submit" form="travel" name="calculate" value="Calculate">
            <br>
        </form>
    </div>
    <div>
        <output form="travel">

        </output>
    </div>

</body>

</html>
